{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Python: FastAPI",
          "type": "python",
          "request": "launch",
          "module": "uvicorn",
          "args": [
              "main:app",
              "--host",
              "0.0.0.0",
              "--port",
              "9090",
              "--reload"
          ],
          "jinja": true,
          "justMyCode": false
      },
      {
          "name": "Python: Current File",
          "type": "python",
          "request": "launch",
          "program": "${file}",
          "console": "integratedTerminal",
          "justMyCode": false
      }
  ]
}